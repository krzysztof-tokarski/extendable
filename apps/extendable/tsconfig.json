{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@core/*": ["./apps/extendable/src/app/core/*"],
      "@environments/*": ["./apps/extendable/src/environments/*"],
      "@features/*": ["./apps/extendable/src/app/features/*"],
      "@shared/*": ["./apps/extendable/src/app/shared/*"],
      "@store/*": ["./apps/extendable/src/app/store/*"],
      "@models/*": ["./libs/api-interfaces/src/lib/*"]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
